{
  "name": "ai-powered-terminal",
  "displayName": "AI-powered terminal",
  "description": "An extension that enhances the readability of output generated by your VSCode terminal.",
  "version": "0.0.3",
  "publisher": "HongweiRuan",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/ai-powered-terminal"
    },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
      "command": "ai-powered-terminal.start",
      "title": "Start AI-Powered Terminal"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
